<!DOCTYPE validators PUBLIC
        "-//Apache Struts//XWork Validator 1.0.2//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
    
	<field name="username">
		<field-validator type="requiredstring">
			<message key="r_message1"></message>
		</field-validator>
	</field>
	
	<field name="code">
		<field-validator type="requiredstring">
			<message key="r_message2"></message>
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="requiredstring">
			<message key="r_message3"></message>
		</field-validator>
	</field>
	
	
</validators>
